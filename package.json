{
  "name": "my-awesome-npm-package-with-bunjs",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "---------- Dev --------------------------------------------------------": "",
    "npmLogin": "bunx npm login",
    "build": "bun build ./src/index.ts --outdir ./dist --format esm && bun build ./src/index.ts --outdir ./dist --format cjs",
    "build:watch": "bun build ./src/index.ts --outdir ./dist --format esm --watch",
    "link": "bun link",
    "unlink": "bun unlink",
    "---------- Versioning --------------------------------------------------": "",
    "version:patch": "bunx bumpp patch --no-push",
    "version:minor": "bunx bumpp minor",
    "version:major": "bunx bumpp major",
    "version:beta": "bunx bumpp preminor --preid beta",
    "version:alpha": "bunx bumpp preminor --preid alpha",
    "version:rc": "bunx bumpp preminor --preid rc",
    "version:prerelease": "bunx bumpp prerelease",
    "---------- Publish -----------------------------------------------------": "",
    "dryRun": "bun publish --dry-run",
    "publish": "bun publish",
    "prepublishOnly": "bun run build",
    "publish:beta": "bun publish --tag beta",
    "publish:alpha": "bun publish --tag alpha",
    "publish:rc": "bun publish --tag rc",
    "---------- Lint -------------------------------------------------------": "",
    "lint": "bunx --bun biome format",
    "lint:fix": "bunx --bun biome format --write",
    "ts": "tsc --noEmit",
    "ts:watch": "npm run ts -- --watch",
    "---------- Test --------------------------------------------------------": "",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "---------- Helper -----------------------------------------------------": "",
    "checkall": "bun run lint:fix && bun run test && bun run ts",
    "deps": "bunx npm-upgrade && bun install",
    "deps:outdated": "bun outdated",
    "-----------------------------------------------------------------------": ""
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/bun": "1.2.19"
  },
  "peerDependencies": {
    "typescript": "5.8.3"
  }
}
